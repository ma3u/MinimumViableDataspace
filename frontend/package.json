{
  "name": "health-ehr2edc-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:local": "vite --port 4000 --mode standalone",
    "build": "tsc && vite build",
    "build:pages": "GITHUB_PAGES=true VITE_API_MODE=mock tsc && GITHUB_PAGES=true vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "generate:types": "npm run generate:edc-types && npm run generate:identity-types && npm run generate:ehr-types",
    "generate:edc-types": "openapi-typescript ../specs/edc-management-api.yaml -o src/types/generated/edc-api.ts",
    "generate:identity-types": "openapi-typescript ../specs/identity-hub-api.yaml -o src/types/generated/identity-hub-api.ts",
    "generate:ehr-types": "openapi-typescript ../specs/ehr-health-api.yaml -o src/types/generated/ehr-api.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:pact": "vitest run --config vitest.pact.config.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "pact:publish": "pact-broker publish ./pacts --consumer-app-version=$npm_package_version --broker-base-url=http://localhost:9292 --broker-username=pact --broker-password=pact"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.0"
  },
  "devDependencies": {
    "@pact-foundation/pact": "^13.1.4",
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^20.19.27",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^2.1.9",
    "@vitest/ui": "^1.0.4",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "happy-dom": "^12.10.3",
    "openapi-typescript": "^7.4.4",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vitest": "^2.1.8"
  }
}
