# Frontend Environment Configuration
# Copy this to .env.local and customize

# API Mode: mock | hybrid | full
# - mock: Direct calls to backend-mock (port 3001)
# - hybrid: Calls to backend-edc with mock data + EDC catalog/metadata
# - full: Complete EDC data flow through connectors
VITE_API_MODE=mock

# Mock Backend URL (used in mock mode)
VITE_MOCK_API_URL=http://localhost:3001

# EDC Backend URL (used in hybrid and full modes)
VITE_EDC_API_URL=http://localhost:3002

# Enable debug logging
VITE_DEBUG=false
